<div class="header">
  <div class="container">
    <h1>Messenger</h1>
  </div>
</div>

  <div class="container">
  	<% @messages.each do |message| %> 
			<div class="message"> 
  			<p ><%= message.content %></p> 
  			<p class="time"><%= message.created_at %></p>
			</div> 
		<% end %>
    <div class='messages'>
    <%= link_to 'New Message', "messages/new" %> 
    </div>
  </div>

